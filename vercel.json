{
    "version": 2,  
    "installCommand":"npm install;npm run build;pip install -r requirements.txt",
    "functions":{
        "api/server.py":{ 
            "includeFiles":"../build/**",
            "memory":1024
        }
    },
    "routes": [
        {
          "src": "/(.*)",
          "dest": "api/server.py"
        }
      ]
}