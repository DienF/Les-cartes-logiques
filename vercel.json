{
    "version": 2,
    "builds": [
        {
            "src": "./server.py",
            "use": "@vercel/python"
        },
        {
            "src":"./build/**",
            "use":"@vercel/static"
        }
    ],
    "functions":{
        "api/install.sh":{ 
            "runtime":"vercel-bash" 
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ]
}