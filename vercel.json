{
    "version": 2,
    
    "scripts": {
        "build": "npm install;npm build"
    },
      
    "builds": [
        {
            "src": "./server.py",
            "use": "@vercel/python"
        },
        {
            "src":"./build/**",
            "use":"@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ]
}