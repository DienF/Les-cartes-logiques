{
    "version": 2,
    
    "functions": {
        "api/install.sh": { "runtime": "vercel-dev-runtime" }
    },
      
    "builds": [
        {
            "src": "./server.py",
            "use": "@vercel/python"
        },
        {
            "src":"./build/**",
            "use":"@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ]
}